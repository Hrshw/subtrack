{
    "version": 2,
    "buildCommand": "cd backend && npm install && npm run build",
    "installCommand": "cd backend && npm install",
    "builds": [
        {
            "src": "backend/src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/src/index.ts"
        }
    ]
}